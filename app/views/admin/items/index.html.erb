<div>
  <h1>商品一覧</h1>

  <p><% @items.each do |item| %></p>
  <%= image_tag item.get_image %>
  <p>商品名</p>
  <%= link_to admin_item_path(item.id) do %>
  <span><%= item.name %></span>
  <% end %>
  <p>税抜価格</p>
  <span>¥<%= item.porice %></span>

  <p>i</p>
  <span><%= item.is_active %></span>

  <% end %>

</div>