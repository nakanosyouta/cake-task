<div>
  <h1>会員一覧</h1>
  <%  @customers.each do |customer| %>
  
   <p>会員ID</p>
   <span><%= customer.id %></span>

   <p>名前</p>
   <%= link_to admin_customer_path(customer.id) do %>
     <%= customer.last_name %>
     <%= customer.first_name %>
   <% end %>

   <p>メールアドレス</p>
   <span><%= customer.email %></span>

   <p>ステータス</p>
   <span><%= customer.is_deleted  ? "退会" : "入会" %></span>
   <h2>--------------------</h2>
  <% end %>
</div>